using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace P04.SortNumbers
{
    class Program
    {
        static void Main(string[] args)
        {
            List<double> dList = Console.ReadLine().Split().Select(double.Parse).ToList();

            dList.Sort();

            Console.WriteLine(string.Join(" <= ", dList));
        }
    }
}
